{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "outDir": "./dist",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [ "./node_modules/@types" ],
    "noImplicitAny": false,
  },
  "include": ["src", "**/*"],
  "exclude": ["node_modules", "dist"],
}